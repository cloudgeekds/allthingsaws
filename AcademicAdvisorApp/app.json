{
  "expo": {
    "name": "AcademicAdvisorApp",
    "slug": "academicadvisorapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "academicadvisorapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F68D11"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.awsomeu.academicadvisorapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F68D11"
      },
      "newArchEnabled": true,
      "package": "com.awsomeu.academicadvisorapp"
    }
  }
}
